{
  "name": "dag-map",
  "version": "2.0.2",
  "description": "A topologically ordered map of key/value pairs with a simple API for adding constraints.",
  "keywords": ["dag", "dependencies", "map"],
  "repository": {
    "type": "git",
    "url": "https://github.com/krisselden/dag-map.git"
  },
  "license": "MIT",
  "author": "Kris Selden",
  "files": [
    "dag-map.d.ts",
    "dag-map.js",
    "dag-map.js.map",
    "dag-map.umd.js",
    "dag-map.umd.js.map"
  ],
  "type": "module",
  "main": "dag-map.umd.js",
  "module": "dag-map.js",
  "jsnext:main": "dag-map.js",
  "typings": "dag-map.d.ts",
  "exports": {
    ".": {
      "import": "./dag-map.js",
      "browser": "./dag-map.umd.js",
      "require": "./dag-map.umd.js"
    }
  },
  "scripts": {
    "build": "tsc && rollup -c",
    "prepare": "npm run build",
    "rollup": "rollup -c",
    "test": "node tests/run.js",
    "tsc": "tsc"
  },
  "devDependencies": {
    "qunitjs": "^2.0.1",
    "rollup": "^0.67.4",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  },
  "readmeFilename": "README.md"
}
